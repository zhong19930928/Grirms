package com.yunhu.yhshxc.activity.carSales.bo;

/**
 * 促销条件
 * @author shenji
 *
 */
public class CarSalesPromotion {
	private int id;
	private int promotionId;//促销数据ID
	private String name;//促销名称 "例如：买100送10"
	private double mCnt;//主产品数量需要满足的条件
	private	double sCnt;//赠送产品数量
	private double amount;//主产品金额需要满足的条件
	private double disAmount;//返还现金金额
	private int preType;//促销类型 1单品买满数量 2单品买满金额 3分类买满数量 4分类买满金额 5总和买满数量 6总和买满金额
	private int disType;//3打折 1送赠品 2 减免金额
	private int isDouble;//0或空不享受多重优惠（满足多个条件时只能选择其一） 1享受多重优惠（满足多个条件时本促销也可享受）
	private String fTime;//有效期开始时间
	private String tTime;//有效期结束时间
	private String disRate;//折扣率
	private String level;//客户级别
	private String mark;//促销说明
	private String orgId;//机构ID
	private String mTab;//主产品表名
	private String mId;//主产品数据ID
	private String mUid;//主产品单位ID
	private String sTab;//赠送产品表名
	private String sId;//赠送产品数据ID
	private String sUid;//赠送产品单位ID
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getPromotionId() {
		return promotionId;
	}
	public void setPromotionId(int promotionId) {
		this.promotionId = promotionId;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getmCnt() {
		return mCnt;
	}
	public void setmCnt(double mCnt) {
		this.mCnt = mCnt;
	}
	public double getsCnt() {
		return sCnt;
	}
	public void setsCnt(double sCnt) {
		this.sCnt = sCnt;
	}
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	public int getPreType() {
		return preType;
	}
	public void setPreType(int preType) {
		this.preType = preType;
	}
	public int getDisType() {
		return disType;
	}
	public void setDisType(int disType) {
		this.disType = disType;
	}
	public int getIsDouble() {
		return isDouble;
	}
	public void setIsDouble(int isDouble) {
		this.isDouble = isDouble;
	}
	public String getfTime() {
		return fTime;
	}
	public void setfTime(String fTime) {
		this.fTime = fTime;
	}
	public String gettTime() {
		return tTime;
	}
	public void settTime(String tTime) {
		this.tTime = tTime;
	}
	public String getDisRate() {
		return disRate;
	}
	public void setDisRate(String disRate) {
		this.disRate = disRate;
	}
	public double getDisAmount() {
		return disAmount;
	}
	public void setDisAmount(double disAmount) {
		this.disAmount = disAmount;
	}
	public String getLevel() {
		return level;
	}
	public void setLevel(String level) {
		this.level = level;
	}
	public String getMark() {
		return mark;
	}
	public void setMark(String mark) {
		this.mark = mark;
	}
	
	public String getOrgId() {
		return orgId;
	}
	public void setOrgId(String orgId) {
		this.orgId = orgId;
	}
	public String getmTab() {
		return mTab;
	}
	public void setmTab(String mTab) {
		this.mTab = mTab;
	}
	public String getmId() {
		return mId;
	}
	public void setmId(String mId) {
		this.mId = mId;
	}
	public String getmUid() {
		return mUid;
	}
	public void setmUid(String mUid) {
		this.mUid = mUid;
	}
	public String getsTab() {
		return sTab;
	}
	public void setsTab(String sTab) {
		this.sTab = sTab;
	}
	public String getsId() {
		return sId;
	}
	public void setsId(String sId) {
		this.sId = sId;
	}
	public String getsUid() {
		return sUid;
	}
	public void setsUid(String sUid) {
		this.sUid = sUid;
	}
	@Override
	public String toString() {
		return "Order3Promotion [id=" + id + ", promotionId=" + promotionId
				+ ", name=" + name + ", mCnt=" + mCnt + ", sCnt=" + sCnt
				+ ", amount=" + amount + ", disAmount=" + disAmount
				+ ", preType=" + preType + ", disType=" + disType
				+ ", isDouble=" + isDouble + ", fTime=" + fTime + ", tTime="
				+ tTime + ", disRate=" + disRate + ", level=" + level
				+ ", mark=" + mark + ", orgId=" + orgId + ", mTab=" + mTab
				+ ", mId=" + mId + ", mUid=" + mUid + ", sTab=" + sTab
				+ ", sId=" + sId + ", sUid=" + sUid + "]";
	}
	
	
}
