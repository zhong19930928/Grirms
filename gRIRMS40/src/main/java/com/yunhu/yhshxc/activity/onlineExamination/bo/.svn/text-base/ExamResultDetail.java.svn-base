package com.yunhu.yhshxc.activity.onlineExamination.bo;

/**
 * 考试结果详细
 * @author jishen
 *
 */
public class ExamResultDetail {
	private int id;//考试结果详细ID
	private int resultId;//结果ID
	private int questionId;//问题ID
	private String optionIds;//选择题的选项ID （多选时的格式，选项ID的链接 101，102，103）
	private String fillContent;//填写内容 开放题存储回答内容，格式AAA||BBB||CCC用两个竖线分割
	private String attachmentUrl;//附件URL 格式 url1,url2,url3
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getResultId() {
		return resultId;
	}
	public void setResultId(int resultId) {
		this.resultId = resultId;
	}
	public int getQuestionId() {
		return questionId;
	}
	public void setQuestionId(int questionId) {
		this.questionId = questionId;
	}
	public String getOptionIds() {
		return optionIds;
	}
	public void setOptionIds(String optionIds) {
		this.optionIds = optionIds;
	}
	public String getFillContent() {
		return fillContent;
	}
	public void setFillContent(String fillContent) {
		this.fillContent = fillContent;
	}
	public String getAttachmentUrl() {
		return attachmentUrl;
	}
	public void setAttachmentUrl(String attachmentUrl) {
		this.attachmentUrl = attachmentUrl;
	}
	
	
}
