package com.yunhu.yhshxc.activity.onlineExamination.bo;

/**
 * 考试问题
 * @author jishen
 *
 */
public class ExamQuestion {
	private int id;
	private int questionId;//问题ID
	private int paperId;//试卷ID
	private int paperNum;//试卷编号
	private int questionLevel;//试题层级
	private int questionsDif;//试题区分
	private String topic;//试题题干
	private String remarks;//备注说明
	private int mulChoiceStandard;//多选题评分标准 1完全符合 2部分符合 3错选扣分
	private String answer;//参考答案
	private int score;//分值
	
	
	public int getMulChoiceStandard() {
		return mulChoiceStandard;
	}
	public void setMulChoiceStandard(int mulChoiceStandard) {
		this.mulChoiceStandard = mulChoiceStandard;
	}
	public String getAnswer() {
		return answer;
	}
	public void setAnswer(String answer) {
		this.answer = answer;
	}
	
	
	
	public int getScore() {
		return score;
	}
	public void setScore(int score) {
		this.score = score;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getQuestionId() {
		return questionId;
	}
	public void setQuestionId(int questionId) {
		this.questionId = questionId;
	}
	public int getPaperId() {
		return paperId;
	}
	public void setPaperId(int paperId) {
		this.paperId = paperId;
	}
	public int getPaperNum() {
		return paperNum;
	}
	public void setPaperNum(int paperNum) {
		this.paperNum = paperNum;
	}
	public int getQuestionLevel() {
		return questionLevel;
	}
	public void setQuestionLevel(int questionLevel) {
		this.questionLevel = questionLevel;
	}
	public int getQuestionsDif() {
		return questionsDif;
	}
	public void setQuestionsDif(int questionsDif) {
		this.questionsDif = questionsDif;
	}
	public String getTopic() {
		return topic;
	}
	public void setTopic(String topic) {
		this.topic = topic;
	}
	public String getRemarks() {
		return remarks;
	}
	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}
	
	
}
