package com.yunhu.yhshxc.nearbyVisit.bo;

import com.yunhu.yhshxc.bo.Func;

public class NearbyStyle {
	public static final int NEARBY_STYLE_EDIT = 1;//输入框
	public static final int NEARBY_STYLE_SPINNER = 2;//下拉框
	public static final int NEARBY_STYLE_DATE = 3;//日期
	public static final int NEARBY_STYLE_EDIT_RANGE = 4;//范围输入框
	public static final int NEARBY_STYLE_MULTI_SPINNER = 5;//多选输入框
	
	private String mark;//标识
	private String name;//显示名称
	private int type;//类型
	private Func func;
	
	public String getMark() {
		return mark;
	}
	public void setMark(String mark) {
		this.mark = mark;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public Func getFunc() {
		return func;
	}
	public void setFunc(Func func) {
		this.func = func;
	}
}
