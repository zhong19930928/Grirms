package com.yunhu.yhshxc.wechat.exchange;

import java.util.HashMap;
import java.util.List;

public class ExchangeWechat {
	private List<HashMap<String, Object>> avatar;// 头像
	private String name = "";// 名字
	private String content = "";// 内容
	private Boolean ifMyself = false;//是否为本人
	



	public Boolean getIfMyself() {
		return ifMyself;
	}

	public void setIfMyself(Boolean ifMyself) {
		this.ifMyself = ifMyself;
	}

	public List<HashMap<String, Object>> getAvatar() {
		return avatar;
	}

	public void setAvatar(List<HashMap<String, Object>> avatar) {
		this.avatar = avatar;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

}
