<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    android:background="@color/white">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">
        <RelativeLayout
            android:id="@+id/rl_top_bookroom"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/app_color"
            android:gravity="center_vertical"
            android:layout_marginBottom="5dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:textSize="20sp"
                android:textColor="@color/white"
                android:layout_centerInParent="true"
                android:text="会议详情" />
            <ImageView
                android:id="@+id/iv_back_bookdetails"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:src="@drawable/back" />
        </RelativeLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/rl_top_bookroom">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/meetingroom_name_meetdetails"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="10dp"
                    android:text="A-2F-27会议室"
                    android:textSize="16sp"
                    android:textColor="@color/black"/>
                <TextView
                    android:id="@+id/meetingroom_bumber_meetdetails"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/meetingroom_name_meetdetails"
                    android:layout_alignBaseline="@+id/meetingroom_name_meetdetails"
                    android:text="规格：8人"
                    android:textSize="14sp"
                    android:textColor="@color/bbs_menu_blue"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_below="@+id/meetingroom_name_meetdetails"
                    android:layout_height="0.5dp"
                    android:background="@color/visit_item_order_disable"/>
                <RelativeLayout
                    android:id="@+id/rl_select_meettime_detail"
                    android:layout_below="@+id/meetingroom_name_meetdetails"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:gravity="center_vertical"
                    >
                    <TextView
                        android:id="@+id/tv_select_meettime_detail"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@null"
                        android:layout_marginRight="15dp"
                        android:text="2018-08-28 10:00-12:30"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="15dp"
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical|left"
                        android:singleLine="true"
                        android:textColor="@color/bbs_menu_blue"
                        android:textSize="15sp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:textSize="13sp"
                        android:layout_marginLeft="5dp"
                        android:textColor="@color/black"
                        android:visibility="gone"
                        android:text="会议时间:"
                        android:layout_alignTop="@+id/tv_select_meettime_detail"
                        android:layout_toRightOf="@+id/iv_select_meettime"
                        android:layout_toEndOf="@+id/iv_select_meettime" />
                    <ImageView
                        android:id="@+id/iv_select_meettime"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:visibility="gone"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/select_meetingtime" />
                </RelativeLayout>
                <View
                    android:id="@+id/line_meeting1"
                    android:layout_width="match_parent"
                    android:layout_below="@+id/rl_select_meettime_detail"
                    android:layout_height="0.5dp"
                    android:background="@color/visit_item_order_disable"/>
                <TextView
                    android:id="@+id/et_meetsubject_meetdetails"
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="18dp"
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:textColorHint="@color/order_title_text"
                    android:background="@null"
                    android:gravity="top"
                    android:layout_below="@+id/line_meeting1"
                    android:hint="请输入您的会议主题"/>
                <View
                    android:id="@+id/line_meeting20"
                    android:layout_width="match_parent"
                    android:layout_below="@+id/et_meetsubject_meetdetails"
                    android:layout_height="0.5dp"
                    android:background="@color/visit_item_order_disable"/>

                <RelativeLayout
                    android:id="@+id/rl_commmitname"
                    android:layout_below="@+id/et_meetsubject_meetdetails"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_height="60dp"
                    android:gravity="center_vertical"
                    >
                    <TextView
                        android:id="@+id/tv"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:gravity="center_vertical"
                        android:textSize="15sp"
                        android:textColor="#333333"
                        android:text="会议发起人：" />
                    <TextView
                        android:id="@+id/tv_commitname"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/tv"
                        android:text="zhonghuibin"
                        android:textColor="#333333"
                        android:textSize="15sp" />
                </RelativeLayout>
                <View
                    android:id="@+id/line_meeting2"
                    android:layout_width="match_parent"
                    android:layout_below="@+id/rl_commmitname"
                    android:layout_height="0.5dp"
                    android:background="@color/visit_item_order_disable"/>

                <RelativeLayout
                    android:id="@+id/rl_askname_bookdetails"
                    android:layout_below="@+id/line_meeting2"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="15dp"
                    android:layout_height="40dp"
                    android:gravity="center_vertical"
                    >
                    <ImageView
                        android:id="@+id/iv_askname_bookdetails"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="20dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginLeft="100dp"
                        android:gravity="center_vertical|right"
                        android:src="@drawable/meeting_addpeople"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:textSize="15sp"
                        android:textColor="#333333"
                        android:text="参会人员:" />
                    />
                </RelativeLayout>

                <GridView
                    android:id="@+id/gridview_people_meeting"
                    android:orientation="horizontal"
                    android:clickable="false"
                    android:layout_below="@+id/rl_askname_bookdetails"
                    android:numColumns="5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                </GridView>

                <View
                    android:id="@+id/line_meeting3"
                    android:layout_width="match_parent"
                    android:layout_below="@+id/gridview_people_meeting"
                    android:layout_marginTop="8dp"
                    android:layout_height="0.5dp"
                    android:background="@color/visit_item_order_disable"/>

                <LinearLayout
                    android:id="@+id/rl_noticeway_bookdetails"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dp"
                    android:layout_below="@+id/line_meeting3"
                    android:layout_height="50dp"
                    android:gravity="center_vertical"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:textSize="15sp"
                        android:textColor="#333333"
                        android:text="发送方式:" />
                    <TextView
                        android:id="@+id/tv_noticeway_email_bookdetails"
                        android:layout_width="55dp"
                        android:layout_height="25dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="28dp"
                        android:background="@drawable/circle_shape_bg_bbs_shadow"
                        android:layout_centerVertical="true"
                        android:visibility="gone"
                        android:text="邮件"
                        android:gravity="center"
                        android:textColor="@color/gray_sub"
                        android:textSize="13sp" />
                    <TextView
                        android:id="@+id/tv_noticeway_message_bookdetail"
                        android:layout_width="55dp"
                        android:layout_height="25dp"
                        android:gravity="center"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="20dp"
                        android:visibility="gone"
                        android:background="@drawable/circle_shape_bg_bbs_shadow"
                        android:layout_centerVertical="true"
                        android:textColor="@color/gray_sub"
                        android:text="短信"
                        android:textSize="13sp" />
                    <TextView
                        android:id="@+id/tv_noticeway_netout_bookdetail"
                        android:layout_width="55dp"
                        android:layout_height="25dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="20dp"
                        android:visibility="gone"
                        android:gravity="center"
                        android:background="@drawable/circle_shape_bg_bbs_shadow"
                        android:layout_centerVertical="true"
                        android:textColor="@color/gray_sub"
                        android:text="站内"
                        android:textSize="13sp" />
                    />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_below="@+id/rl_noticeway_bookdetails"
                    android:layout_height="0.5dp"
                    android:background="@color/visit_item_order_disable"/>

                <TextView
                    android:id="@+id/select_service"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12dp"
                    android:text="会议服务:"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@+id/rl_noticeway_bookdetails"
                    android:textColor="@color/order_title_text"/>
                <TextView
                    android:id="@+id/tv_service_chaye"
                    android:layout_width="55dp"
                    android:layout_height="25dp"
                    android:gravity="center"
                    android:background="@drawable/circle_shape_bg_bbs_shadow"
                    android:textColor="@color/gray_sub"
                    android:layout_below="@+id/select_service"
                    android:layout_marginLeft="20dp"
                    android:visibility="gone"
                    android:text="茶叶"
                    android:textSize="13sp" />
                <TextView
                    android:id="@+id/tv_service_chabei"
                    android:layout_width="55dp"
                    android:layout_height="25dp"
                    android:gravity="center"
                    android:background="@drawable/circle_shape_bg_bbs_shadow"
                    android:textColor="@color/gray_sub"
                    android:layout_below="@+id/select_service"
                    android:layout_toRightOf="@+id/tv_service_chaye"
                    android:layout_marginLeft="30dp"
                    android:visibility="gone"
                    android:text="茶杯"
                    android:textSize="13sp" />
                <TextView
                    android:id="@+id/tv_service_kaishui"
                    android:layout_width="55dp"
                    android:layout_height="25dp"
                    android:gravity="center"
                    android:background="@drawable/circle_shape_bg_bbs_shadow"
                    android:textColor="@color/gray_sub"
                    android:layout_below="@+id/select_service"
                    android:layout_toRightOf="@+id/tv_service_chabei"
                    android:visibility="gone"
                    android:layout_marginLeft="30dp"
                    android:text="开水"
                    android:textSize="13sp" />
                <TextView
                    android:id="@+id/tv_service_zhuopan"
                    android:layout_width="55dp"
                    android:layout_height="25dp"
                    android:gravity="center"
                    android:background="@drawable/circle_shape_bg_bbs_shadow"
                    android:textColor="@color/gray_sub"
                    android:layout_below="@+id/select_service"
                    android:visibility="gone"
                    android:layout_toRightOf="@+id/tv_service_kaishui"
                    android:layout_marginLeft="30dp"
                    android:text="桌盘"
                    android:textSize="13sp" />
                <TextView
                    android:id="@+id/tv_service_guopan"
                    android:layout_width="55dp"
                    android:layout_height="25dp"
                    android:gravity="center"
                    android:layout_marginTop="10dp"
                    android:visibility="gone"
                    android:background="@drawable/circle_shape_bg_bbs_shadow"
                    android:textColor="@color/gray_sub"
                    android:layout_below="@+id/tv_service_chaye"
                    android:layout_marginLeft="20dp"
                    android:text="果盘"
                    android:textSize="13sp" />
                <TextView
                    android:id="@+id/tv_service_biaoyu"
                    android:layout_width="55dp"
                    android:layout_height="25dp"
                    android:gravity="center"
                    android:background="@drawable/circle_shape_bg_bbs_shadow"
                    android:textColor="@color/gray_sub"
                    android:visibility="gone"
                    android:layout_marginTop="10dp"
                    android:layout_below="@+id/tv_service_chaye"
                    android:layout_toRightOf="@+id/tv_service_guopan"
                    android:layout_marginLeft="30dp"
                    android:text="标语"
                    android:textSize="13sp" />
                <RelativeLayout
                    android:id="@+id/rl_notic_bookdetails"
                    android:layout_below="@+id/tv_service_guopan"
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="center_vertical"
                    >
                    <TextView
                        android:id="@+id/tv_notic_bookdetails"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@null"
                        android:layout_marginRight="30dp"
                        android:layout_centerVertical="true"
                        android:hint="提前5分钟"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="100dp"
                        android:ellipsize="end"
                        android:gravity="center_vertical|right"
                        android:maxWidth="230dp"
                        android:singleLine="true"
                        android:textColor="#333333"
                        android:textSize="13sp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        android:textSize="15sp"
                        android:textColor="#333333"
                        android:text="会前提醒" />
                    <ImageView
                        android:layout_width="5dp"
                        android:layout_height="10dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="20dp"
                        android:background="@drawable/meeting_right" />
                    />
                    <View
                        android:layout_width="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_height="0.5dp"
                        android:background="@color/visit_item_order_disable"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_alignParentTop="true"
                        android:layout_height="0.5dp"
                        android:background="@color/visit_item_order_disable"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_renumber_bookdetails"
                    android:layout_below="@+id/rl_notic_bookdetails"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:gravity="center_vertical"
                    >
                    <TextView
                        android:id="@+id/tv_renumber_bookdetails"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@null"
                        android:layout_marginRight="30dp"
                        android:layout_centerVertical="true"
                        android:hint="永不"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="100dp"
                        android:ellipsize="end"
                        android:gravity="center_vertical|right"
                        android:maxWidth="230dp"
                        android:singleLine="true"
                        android:textColor="#333333"
                        android:textSize="13sp" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:text=""
                        android:textSize="15sp"
                        android:textColor="#333333"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_alignParentBottom="true"
                        android:layout_height="0.5dp"
                        android:background="@color/visit_item_order_disable"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="15dp"
                        android:text="重复"
                        android:textSize="15sp"
                        android:textColor="#333333"/>

                    <ImageView
                        android:layout_width="5dp"
                        android:layout_height="10dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="20dp"
                        android:background="@drawable/meeting_right" />
                    />
                </RelativeLayout>

                <TextView
                    android:id="@+id/tv_beizhu_meetdetails"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@+id/rl_renumber_bookdetails"
                    android:text="备注"
                    android:textSize="15sp"
                    android:textColor="#333333"/>
                <EditText
                    android:id="@+id/et_beizhu_meetdetails"
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:textSize="12sp"
                    android:padding="10dp"
                    android:visibility="gone"
                    android:textColor="@color/black"
                    android:background="@drawable/circle_shape_bg_bbs_while"
                    android:gravity="top"
                    android:layout_below="@+id/tv_beizhu_meetdetails"/>
                <TextView
                    android:id="@+id/tv_prieve"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@+id/et_beizhu_meetdetails"
                    android:textStyle="bold"
                    android:textColor="#666666"
                    android:textSize="16sp"
                    android:text="暂缓"/>
                <TextView
                    android:id="@+id/tv_acccess"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@+id/tv_prieve"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@+id/et_beizhu_meetdetails"
                    android:textStyle="bold"
                    android:textColor="#666666"
                    android:layout_marginRight="30dp"
                    android:textSize="16sp"
                    android:text="接受"/>
                <TextView
                    android:id="@+id/tv_refuse"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/tv_prieve"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@+id/et_beizhu_meetdetails"
                    android:textStyle="bold"
                    android:textColor="#666666"
                    android:layout_marginLeft="30dp"
                    android:textSize="16sp"
                    android:text="拒绝"/>

                <RelativeLayout
                    android:id="@+id/rl_bookmeetdetails_cancle"
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_below="@+id/tv_acccess"
                    android:gravity="center"
                    android:background="@drawable/circle_shape_bg_bbs_menu_bule"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="30dp"
                    android:layout_centerHorizontal="true">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="18sp"
                        android:textColor="@color/white"
                        android:text="取消会议"
                        />
                </RelativeLayout>
            </RelativeLayout>
            <TextView
                android:id="@+id/rl_back"
                android:background="#00000000"
                android:layout_marginBottom="80dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            </TextView>
        </FrameLayout>
    </RelativeLayout>
</ScrollView>